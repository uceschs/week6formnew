<?xml version='1.0' encoding='utf-8'?>
<widget id="FormApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>The Question App</name>
    <description>
        The Question App allows you to track a location on a map and obtain the coordinate for the qestion you want to give it to user.
    </description>
    <author email="uceschs@ucl.ac.uk" href="https://github.com/uceschs">
        Chiao Ju, Hsueh
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
